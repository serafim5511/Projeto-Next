@model Projeto_Next.Models.DadosPessoais


@using (Html.BeginForm("Logar", "Home", FormMethod.Get))
{
    <h1>Entrar</h1>
    <div class="form-group md-form">
        <input asp-for="Email" type="text" name="Email" id="Email" placeholder="Email" class="form-control validate" />
    </div>
    <div class="form-group md-form">
        @Html.TextBoxFor(txt => txt.Senha, new { @class = "form-control", @required = "required", @name = "SenhaCliente", @id = "Senha", @placeholder = "Senha", @type = "password" })
    </div>
    @if(ViewBag.autenticar != null)
    {
    <div class="text-danger">@ViewBag.autenticar</div>
    }
    <div class="form-group md-form">
        <button type="submit" class="btn btn-success">Entrar</button>
    </div>
}
